import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
         float a=s.nextFloat();
        System.out.printf("%d\n",(int)a);
        System.out.println((float)Math.ceil(a));
        System.out.println((float)Math.floor(a));
    }
}
