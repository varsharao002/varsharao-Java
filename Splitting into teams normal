import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int n,t;
        Scanner s=new Scanner(System.in);
            n=s.nextInt();
        t=s.nextInt();
        int r1=n/t;
        int r2=n%t;
        System.out.println("The number of students in each team is "+r1+" and left out is "+r2);

    }
