import java.util.*;
class Test
{
public static void main(String[] arg) 
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
{
    a[i]=s.nextInt();
}
int cnt=1;
int max=a[0];
for(int i=0;i<n;i++)
{
    if(max>a[i])
    {
        cnt++;
    }
}
System.out.print(cnt);
}
}
