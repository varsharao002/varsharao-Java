import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int x1=s.nextInt();
        int y1=s.nextInt();
        int x2=s.nextInt();
        int y2=s.nextInt();
        int x3=s.nextInt();
        int y3=s.nextInt();
        float r1=(float)(x1+x2+x3)/3;
        float r2=(y1+y2+y3)/3;
        System.out.printf("%.1f\n%.1f",r1,r2);
        
        
    }
}
