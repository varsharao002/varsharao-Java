import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        long a=s.nextLong();
         long b=s.nextLong();
        if(a<b)
            System.out.println(b);
        else
            System.out.println(a);

    }
}
